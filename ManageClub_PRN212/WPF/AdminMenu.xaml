<Window x:Class="ManageClub_PRN212.WPF.AdminMenu"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ManageClub_PRN212.WPF"
        mc:Ignorable="d"
        Title="AdminMenu" Height="300" Width="300" WindowStartupLocation="CenterScreen">
    <Grid Background="LightBlue">
        <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center">
            <Label Name="lbName" Content="Welcome " Foreground="Red" FontSize="20" FontWeight="Bold"/>
            <Button Content="Club Management" Height="30" Width="150" Margin="10"/>
            <Button Content="Account Management" Height="30" Width="150" Margin="10"/>
            <Button Content="My Profile" Height="30" Width="150" Margin="10"/>
        </StackPanel>
    </Grid>
</Window>
